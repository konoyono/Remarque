{
  "manifest_version": 2,
  "name": "Remarque",
  "version": "0.0.7",
  "description": "Remarque is a multi files note tool in blank tab.",
  "icons": {
    "16": "remarque.png",
    "48": "remarque.png",
    "128": "remarque.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://cdn.jsdelivr.net/npm/vue@2/dist/vue.js; object-src 'self'"
}